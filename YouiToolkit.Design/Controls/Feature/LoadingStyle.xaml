                                      Y2="0"
                                      Stroke="{TemplateBinding Foreground}"
                                      StrokeThickness="{Binding ActualWidth, Converter={StaticResource LoadingBackgroundThicknessConverter}, RelativeSource={RelativeSource AncestorType=local:Loading}}"
