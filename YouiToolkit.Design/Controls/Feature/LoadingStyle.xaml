<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:YouiToolkit.Design">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/YouiToolkit.Design;component/Resources/Resource.xaml" />
        <ResourceDictionary Source="pack://application:,,,/YouiToolkit.Design;component/Resources/Converter.xaml" />
    </ResourceDictionary.MergedDictionaries>
    
    <Style TargetType="{x:Type local:Loading}">
        <Setter Property="SnapsToDevicePixels"
                Value="{StaticResource Default_SnapsToDevicePixels}" />
        <Style.Triggers>
            <Trigger Property="LoadingStyle"
